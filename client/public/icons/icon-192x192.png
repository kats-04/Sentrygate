<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" width="512" height="512">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e40af;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="256" cy="256" r="256" fill="url(#grad1)"/>
  
  <!-- Dashboard icon (bar chart) -->
  <g transform="translate(100, 150)">
    <!-- Chart bars -->
    <rect x="20" y="180" width="40" height="80" fill="#ffffff" rx="5"/>
    <rect x="80" y="120" width="40" height="140" fill="#e0e7ff" rx="5"/>
    <rect x="140" y="80" width="40" height="180" fill="#ffffff" rx="5"/>
    <rect x="200" y="140" width="40" height="120" fill="#e0e7ff" rx="5"/>
    
    <!-- Grid lines -->
    <line x1="0" y1="260" x2="260" y2="260" stroke="#ffffff" stroke-width="2" opacity="0.3"/>
    <line x1="0" y1="200" x2="260" y2="200" stroke="#ffffff" stroke-width="2" opacity="0.2"/>
    <line x1="0" y1="140" x2="260" y2="140" stroke="#ffffff" stroke-width="2" opacity="0.2"/>
  </g>
  
  <!-- User icon overlay -->
  <g transform="translate(320, 80)">
    <circle cx="50" cy="50" r="35" fill="#ffffff"/>
    <path d="M 20 120 Q 20 100 50 100 Q 80 100 80 120 L 80 150 L 20 150 Z" fill="#ffffff"/>
  </g>
</svg>
